{
  "compilerOptions": {
    "baseUrl": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "CommonJS",
    "target": "ES2018",
    "lib": [
      "DOM",
      "ES2018"
    ],
    "allowJs": false,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": [
      "obsidian"
    ],
    "skipLibCheck": true
  },
  "include": [
    "main.ts"
  ]
}
